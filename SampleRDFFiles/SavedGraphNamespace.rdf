<?xml version="1.0"?>

<rdf:RDF xml:lang="en"
	 xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#" 
	 xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"> 
	
	<rdfs:Class rdf:ID="Graph">
		<rdfs:comment>This class represents the graph</rdfs:comment>
	</rdfs:Class>
	
	<rdfs:Class rdf:ID="Node">
		<rdfs:comment>This class represents the visual objects of nodes in graph</rdfs:comment>
	</rdfs:Class>

	<rdf:Property rdf:ID="name">
		<rdfs:domain rdf:resource="#Node"/>
		<rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
		<rdfs:comment>The name of this node</rdfs:comment>
	</rdf:Property>
	
	<rdf:Property rdf:ID="coordX">
		<rdfs:domain rdf:resource="#Node"/>
		<rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
		<rdfs:comment>The X coordinate of this node</rdfs:comment>
	</rdf:Property>

	<rdf:Property rdf:ID="coordY">
		<rdfs:domain rdf:resource="#Node"/>
		<rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
		<rdfs:comment>The Y coordinate of this node</rdfs:comment>
	</rdf:Property>

	<rdf:Property rdf:ID="nailed">
		<rdfs:domain rdf:resource="#Node"/>
		<rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
		<rdfs:comment>Nailed status of this node</rdfs:comment>
	</rdf:Property>
	
	<rdf:Property rdf:ID="visible">
		<rdfs:domain rdf:resource="#Node"/>
		<rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
		<rdfs:comment>Visibility status of this node</rdfs:comment>
	</rdf:Property>
	
	<rdf:Property rdf:ID="rank">
		<rdfs:domain rdf:resource="#Node"/>
		<rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
		<rdfs:comment>Rank of this node</rdfs:comment>
	</rdf:Property>
	
</rdf:RDF>

