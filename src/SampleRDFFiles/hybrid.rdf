<iCalendar:Parameter rdf:ID="RSVP">
	<rdfs:comment>RSVP Expectation</rdfs:comment>
<!--	<rdfs:domain rdf:resource="#ATTENDEE"/> -->
<!-- 
LM: another three-way relationship - the RSVP expectation from the 
cal-user as regards the event

MHA: actually, this and the previous ROLE can only be applied to attendees 
(not organizer, for example)
-->
	<rdfs:range rdf:resource="#BOOLEAN"/>
</iCalendar:Parameter>


<iCalendar:Parameter rdf:ID="SENT-BY">
