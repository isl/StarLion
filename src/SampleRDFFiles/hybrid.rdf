<rdfs:domain rdf:resource="#RECUR" />
<rdfs:range rdf:resource="#INTEGER" />
<!-- INTEGER sign represents from the beginning/end of the set.
Maybe should be a bag -->
</rdf:Property>


<!-- Alarm component properties -->



<rdf:Property rdf:ID="ACTION"
 rdfs:label="Action" 
 rdfs:comment="The action to be invoked when the alarm is triggered. Must
be specified once in a VALARM">
<rdfs:domain rdf:resource="#VALARM" />
<rdfs:range rdf:resource="#TEXT" />
<!-- 
AUDIO
DISPLAY
EMAIL
PROCEEDURE
LM: maybe should be literal

MHA: Actually, this is a bit more complicated since each action 
denotes a different kind of alarm, really.  Perhaps it should be 
folded into subclasses of VALARM?
-->
</rdf:Property>



<rdf:Property rdf:ID="REPEAT"
 rdfs:label="Repeat Count" 
 rdfs:comment="The number of times the alarm should be repeated after the
initial trigger">
<rdfs:domain rdf:resource="#VALARM" />
<rdfs:range rdf:resource="#INTEGER" />
<!-- 
sign ignored
-->
</rdf:Property>


<iCalendar:Property rdf:ID="TRIGGER"
 rdfs:label="Trigger" 
 rdfs:comment="When an alarm is triggered">
<rdfs:domain rdf:resource="#VALARM" />
<!-- 
LM: but range can also be a DATE-TIME (UTC). The relationship between these
needs more specification. The duration is used because it is the positive or
negative distance in time from the beginning or end of the event.
see also #RELATED
MHA: Ok, how about we subclass the TRIGGER (REL-TRIGGER and ABS-TRIGGER), 
since they have different parameters.
-->
</iCalendar:Property>

<iCalendar:Property rdf:ID="REL-TRIGGER"
 rdfs:label="Relative Trigger"
 rdfs:comment="When an alarm is triggered -- relative to the associated event
 or todo">
 <!-- Correction: Since TRIGGER has been defined as a Property, the
subPropertyOf should be used -->
<!--	<rdfs:subClassOf rdf:resource="#TRIGGER"/> -->
<rdfs:domain rdf:resource="#VALARM" />
 <rdfs:subPropertyOf rdf:resource="#TRIGGER"/>
 <rdfs:range rdf:resource="#DURATION"/>
</iCalendar:Property>

<iCalendar:Property rdf:ID="ABS-TRIGGER"
 rdfs:label="Absolute Trigger"
 rdfs:comment="When an alarm is triggered -- date and time or just date.">
<!-- Correction: Since TRIGGER has been defined as a Property, the
subPropertyOf should be used -->
<!--	<rdfs:subClassOf rdf:resource="#TRIGGER"/> -->
 <rdfs:subPropertyOf rdf:resource="#TRIGGER"/>
<rdfs:domain rdf:resource="#VALARM" />
<rdfs:range rdf:resource="#TimeEntry"/>
</iCalendar:Property>



<!-- change management component properties -->



<iCalendar:Property rdf:ID="CREATED"
 rdfs:label="Date/time created" 
 rdfs:comment="Date/time a CAL-COMPONENT was created (not VFREEBUSY) (UTC)">
<rdfs:domain rdf:resource="#CAL-COMPONENT" /> 
<rdfs:range rdf:resource="#DATE-TIME" />
</iCalendar:Property>	


<iCalendar:Property rdf:ID="DTSTAMP"
 rdfs:label="Date/time stamp" 
 rdfs:comment="Date/time a VCALENDAR was created (UTC)">
<rdfs:domain rdf:resource="#VCALENDAR" />
<rdfs:range rdf:resource="#DATE-TIME" />
</iCalendar:Property>	


<iCalendar:Property rdf:ID="LAST-MODIFIED"
 rdfs:label="Date/time last modified" 
 rdfs:comment="Date/time a CAL-COMPONENT was last modified (not VFREEBUSY) 
(UTC)">
 <!-- 
 why not VFREEBUSY? 
 -->
 <rdfs:domain rdf:resource="#CAL-COMPONENT"/> 
 <rdfs:domain rdf:resource="#VTIMEZONE" />
 <rdfs:range rdf:resource="#DATE-TIME" />
</iCalendar:Property>	



<iCalendar:Property rdf:ID="SEQUENCE"
 rdfs:label="Sequence" 
 rdfs:comment="The revision number of a calendar component wh=ithin a
 sequence of revisions (not VFREEBUSY)">
 <!--
 starts with ); any time the ORGANIZER makes changes to
 DTSTART
 DTEND
 DUE
 RDATE
 RRULE
 EXDATE
 EXRULEW
 STATUS
 the sequence number must be incremented.
 The ORGANIZER can also force an increment is he thinks that a change made
 may jeopardize attendance
 why not VFREEBUSY?
 -->
 <rdfs:domain rdf:resource="#CAL-COMPONENT"/> 
 <rdfs:range rdf:resource="#INTEGER" />
</iCalendar:Property>	


