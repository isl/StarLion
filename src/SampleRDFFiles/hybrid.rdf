</rdf:Property>


<!-- recurrance properties

#FREQ
#COUNT
#UNTIL
#INTERVAL
#BYSECOND
#BYMINUTE
#BYHOUR
#BYMONTHDAY
#BYYEARDAY
#BYWEEKNO
#BYMONTH
#BYSETPOS
#WKSET

-->




<rdf:Property rdf:ID="FREQ"
 rdfs:label="Frequency" 
 rdfs:comment="The type of the recurrance rule">
 <rdfs:domain rdf:resource="#RECUR" />
 <rdfs:range rdf:resource="#TEXT" />
<!--
SECONDLY
MINUTELY
HOURLY
DAILY
WEEKLY
MONTHLY
YEARLY
possibly should be a literal
MHA: or some kind of choice.
-->
</rdf:Property>


<rdf:Property rdf:ID="INTERVAL"
 rdfs:label="Interval" 
 rdfs:comment="How often the recurrance rule repeats. The dfault is 1,
i.e. _every_ year etc">
<rdfs:domain rdf:resource="#RECUR" />
<rdfs:range rdf:resource="#INTEGER" />
<!-- INTEGER sign ignored -->
</rdf:Property>



<rdf:Property rdf:ID="UNTIL"
 rdfs:label="Until" 
 rdfs:comment="Defines a date-time value which bounds the recurrance rule in
an inclusive manner. If a DATE, must be UTC">
<rdfs:domain rdf:resource="#RECUR" />
<rdfs:range rdf:resource="#TimeEntry" />
<!--
LM: Date used as superclass of DATE-TIME
MHA: Changed to TimeEntry
-->
</rdf:Property>


<rdf:Property
rdf:ID="COUNT"
 rdfs:label="Count" 
 rdfs:comment="the number of occurrances at which to range-bound the
recurance. If specified, the DTSTART value counts as the first of these">
<rdfs:domain rdf:resource="#RECUR" />
<rdfs:range rdf:resource="#INTEGER" />
<!-- INTEGER sign ignored -->
</rdf:Property>


<rdf:Property rdf:ID="BYSECOND"
 rdfs:label="By second" 
 rdfs:comment="A list of second values for recurrance">
<rdfs:domain rdf:resource="#RECUR" />
<rdfs:range rdf:resource="#INTEGER" />
<!-- INTEGER sign ignored 0-59 only. maybe should be a bag -->
</rdf:Property>


<rdf:Property rdf:ID="BYMINUTE"
 rdfs:label="By minute" 
 rdfs:comment="A list of minute values for recurrance">
<rdfs:domain rdf:resource="#RECUR" />
<rdfs:range rdf:resource="#INTEGER" />
<!-- INTEGER sign ignored 0-59 only. maybe should be a bag -->
</rdf:Property>


<rdf:Property
rdf:ID="BYHOUR"
 rdfs:label="By hour" 
 rdfs:comment="A list of hour values for recurrance">
<rdfs:domain rdf:resource="#RECUR" />
<rdfs:range rdf:resource="#INTEGER" />
<!-- INTEGER sign ignored 0-23 only. maybe should be a bag -->
</rdf:Property>



<rdf:Property rdf:ID="BYDAY"
 rdfs:label="By day" 
 rdfs:comment="A list of day values for recurrance">
<rdfs:domain rdf:resource="#RECUR" />
<rdfs:range rdf:resource="#TEXT" />

<!-- 
MO
TU
WE
TH
FR
SA
SU
It can also be prefixed by a positive or negative integer representing the
nth occurance of the day from the beginning or end of the month.
So this is looking like a day object or a day of the month object.
Maybe should be literals, maybe should be a bag 
MHA: maybe should be choices
-->
</rdf:Property>


<rdf:Property rdf:ID="BYMONTHDAY"
 rdfs:label="By month day" 
 rdfs:comment="A list of INTEGERS representing days in a month values for
recurrance">
<rdfs:domain rdf:resource="#RECUR" />
<rdfs:range rdf:resource="#INTEGER" />
<!-- INTEGER sign represents from the beginning/end of the month.
 0-31 only. Maybe should be a bag -->
</rdf:Property>



<rdf:Property rdf:ID="BYYEARDAY"
 rdfs:label="By year day" 
 rdfs:comment="A list of INTEGERS representing days in a year values for
recurrance">
<rdfs:domain rdf:resource="#RECUR" />
<rdfs:range rdf:resource="#INTEGER" />
<!-- INTEGER sign represents from the beginning/end of the year.
 0-366 only. Maybe should be a bag -->
</rdf:Property>


<rdf:Property rdf:ID="BYWEEKNO"
 rdfs:label="By week number" 
 rdfs:comment="A list of INTEGERS representing weeks in a year values for
recurrance">
<rdfs:domain rdf:resource="#RECUR" />
<rdfs:range rdf:resource="#INTEGER" />
<!-- INTEGER sign represents from the beginning/end of the year.
 0-53 only. Maybe should be a bag -->
</rdf:Property>



<rdf:Property rdf:ID="BYMONTH"
 rdfs:label="By month" 
 rdfs:comment="A list of INTEGERS representing months in a year values for
recurrance">
<rdfs:domain rdf:resource="#RECUR" />
<rdfs:range rdf:resource="#INTEGER" />
<!-- INTEGER sign represents from the beginning/end of the year.
 0-12 only. Maybe should be a bag -->
</rdf:Property>



<rdf:Property rdf:ID="WKST"
 rdfs:label="Working week start" 
 rdfs:comment="Specifies the day on which the working week starts">
<!-- 
MO
TU
WE
TH
FR
SA
SU
MHA: Should probably be choices.
-->
<rdfs:domain rdf:resource="#RECUR" />
<rdfs:range rdf:resource="#TEXT" />
<!-- 
maybe should be a literal; maybe a bag
-->
</rdf:Property>



<rdf:Property rdf:ID="BYSETPOS"
 rdfs:label="By set position" 
 rdfs:comment="A list of INTEGERS representing the nth occurrance of the
